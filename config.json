      "type": "urltest",
      "tag": "香港节点",
      "providers": ["airport"],
      "includes": ["(?i)(港|HK|Hong Kong)"]
    },
    {
      "type": "urltest",
      "tag": "台湾节点",
      "providers": ["airport"],
      "includes": ["(?i)(台|TW|Taiwan)"]
    },
    {
      "type": "urltest",
      "tag": "日本节点",
      "providers": ["airport"],
