    },
    {
      "type": "urltest",
      "tag": "新加坡节点",
      "providers": ["airport"],
      "includes": ["(?i)(新加坡|SG|Singapore|狮城)"]
    },
    {
      "type": "urltest",
      "tag": "美国节点",
      "providers": ["airport"],
      "includes": ["(?i)(美|US|United States|洛杉矶|圣何塞)"]
    },
    {
      "type": "urltest",
      "tag": "其它地区",
      "providers": ["airport"],
      "includes": ["^(?!.*(港|HK|JP|Japan|US|United States|TW|Taiwan|SG|Singapore|CN|China)).*$"]
