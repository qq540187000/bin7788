    {
      "type": "selector",
      "tag": "负载均衡",
      "providers": ["airport"],
      "includes": ["^(?!.*(套餐|重置|剩余|到期|订阅|群|账户|流量|有效期|时间|官网|失联|余额)).*$"]
    },
